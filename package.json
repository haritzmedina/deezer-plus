{
  "name": "deezer-plus",
  "private": true,
  "version": "0.0.1",
  "description": "This extension adds new functionality to deezer music",
  "scripts": {
    "start": "npm run dev:chrome",
    "build": "npm run build:chrome",
    "build:chrome": "gulp pack --production --vendor=chrome",
    "build:firefox": "gulp pack --production --vendor=firefox",
    "build:opera": "gulp pack --production --vendor=opera",
    "build:edge": "gulp pack --production --vendor=edge",
    "dev": "npm run dev:chrome",
    "dev:chrome": "gulp --watch --vendor=chrome",
    "dev:firefox": "gulp --watch --vendor=firefox",
    "dev:opera": "gulp --watch --vendor=opera",
    "dev:edge": "gulp --watch --vendor=edge",
    "lint": "standard"
  },
  "standard": {
    "globals": [
      "chrome"
    ]
  },
  "dependencies": {
    "axios": "^0.21.0",
    "lodash": "^4.17.20",
    "sweetalert2": "^10.10.4",
    "bootstrap": "^4.5.3"
  },
  "devDependencies": {
    "@babel/register": "^7.12.1",
    "@babel/cli": "^7.12.8",
    "@babel/core": "7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-syntax-async-generators": "^7.8.4",
    "@babel/preset-env": "^7.12.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "chromereload": "0.x.x",
    "core-js": "^3.8.0",
    "debounce": "^1.2.0",
    "del": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "gulp": "^4.0.2",
    "gulp-bump": "^3.2.0",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-filter": "^6.0.0",
    "gulp-git": "^2.10.1",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "^7.1.0",
